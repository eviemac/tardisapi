
<%= form_for(@symptom.user, @symptom) do |f| %>
    <% if @symptom.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@symptom.errors.count, "error") %> prohibited this symptom from being saved:</h2>
          <ul>
            <% @symptom.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
    <% end %>
    <div class="field">
      <%= f.label :'which symptom' %><br />
      <%= f.text_field :symptom %>
    </div>
    <div class="field">
      <%= f.label :'timing' %><br />
      <%= f.text_area :symptom %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>